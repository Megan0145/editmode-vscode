{
  "name": "editmode",
  "displayName": "EditMode",
  "publisher": "Megan Ennis",
  "description": "Create Editmode chunks at the click of a button.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:editmode.createChunk"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "editmode.createChunk",
        "title": "EditMode: Create Chunk"
      }
	], 
	"keybindings": [
		{
			"command": "editmode.createChunk",
			"key": "ctrl+shift+l",
			"mac": "cmd+shift+l"
		}
	]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.44.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
